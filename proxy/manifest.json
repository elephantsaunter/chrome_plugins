   "icons": {
      "128": "images/logo_128.png",
      "16": "images/logo_16.png",
      "48": "images/logo_48.png"
   },
   "manifest_version": 2,
   "minimum_chrome_version": "22.0.0",
   "name": "__MSG_appName__",
